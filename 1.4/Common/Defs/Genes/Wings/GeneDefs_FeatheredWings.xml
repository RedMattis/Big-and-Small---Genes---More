<Defs>

  <!-- Bird Wings -->
  
  <GeneDef ParentName="BS_FunctionalWingBase">
		<defName>BS_MajesticBird</defName>
		<label>Majestic Bird Wings (Large)</label>
		<description>Carriers of this gene possess angelic wings.</description>
		<iconPath>Humanlike/BodyAttachments/BS_GorgonWings_south</iconPath>
		<displayOrderInCategory>777</displayOrderInCategory>
    <geneClass>BetterPrerequisites.PGene</geneClass>
		<graphicData>
			<graphicPath>Humanlike/BodyAttachments/BS_GorgonWingsBack</graphicPath>
			<colorType>Hair</colorType>
      <color>(245,245,245)</color>
      <drawScale>1.9</drawScale>
		</graphicData>
    <modExtensions>
      <li Class="BetterPrerequisites.GeneExtension">
        <applyBodyHediff>
          <li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core">
            <hediff>BS_Floating</hediff>
          </li>
        </applyBodyHediff>
        <hiddenGenes>
          <li>BS_MajesticBird_Front</li>
        </hiddenGenes>
      </li>
      <li MayRequire="pathfinding.framework" Class="PathfindingFramework.MovementExtension">
        <movementDef>PF_Movement_Flying</movementDef>
      </li>
    </modExtensions>
	</GeneDef>

