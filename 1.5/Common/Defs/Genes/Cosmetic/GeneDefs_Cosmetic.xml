  <GeneDef ParentName="GeneNoseBase">
    <defName>BS_WerewolfSnoot</defName>
    <label>werewolf snoot</label>
    <description>Rawr! Carriers of this gene possess a mean werewolf-like snoot.</description>
    <iconPath>GeneIcons/BS_werewolfsnoot</iconPath>
    <selectionWeight>0.1</selectionWeight>
    <symbolPack>
      <prefixSymbols>
        <li>
          <symbol>lycan</symbol>
        </li>
        <li>
          <symbol>lunar</symbol>
        </li>
        <li>
          <symbol>mad</symbol>
        </li>
        <li>
          <symbol>snout</symbol>
        </li>
        <li>
          <symbol>snoot</symbol>
        </li>
        <li>
          <symbol>wolf</symbol>
        </li>
        <li>
          <symbol>moon</symbol>
        </li>
      </prefixSymbols>
      <suffixSymbols>
        <li>
          <symbol>wolf</symbol>
        </li>
        <li>
          <symbol>moon</symbol>
        </li>
        <li>
          <symbol>crazed</symbol>
        </li>
      </suffixSymbols>
    </symbolPack>
    <modExtensions>
      <li MayRequire="OskarPotocki.VanillaFactionsExpanded.Core" Class="VanillaGenesExpanded.GeneExtension">
        <backgroundPathEndogenes>GeneIcons/BS_BackEndogene</backgroundPathEndogenes>
        <backgroundPathXenogenes>GeneIcons/BS_BackXenogene</backgroundPathXenogenes>
      </li>
    </modExtensions>
    <renderNodeProperties Inherit="False">
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <texPath>Humanlike/HeadAttachments/BS_WerewolfSnoot</texPath>
        <drawSize>(1.0,1.0)</drawSize>
        <useSkinShader>true</useSkinShader>
        <colorType>Hair</colorType>
        <drawData>
          <defaultData>
            <layer>52</layer>
          </defaultData>
        </drawData>
        <parentTagDef>Head</parentTagDef>
        <narrowCrownHorizontalOffset>0.03</narrowCrownHorizontalOffset>
      </li>
    </renderNodeProperties>
  </GeneDef>
  <GeneDef ParentName="BS_Hornbase">
    <defName>BS_BananaSplitHair</defName>
    <label>neko banana split horns with sprinkles</label>
    <description>Carriers of this gene grow protuding banana-looking horns, and cream with a cherry-looking thing on top.\n\nThat's can't be right.</description>
    <iconPath>GeneIcons/BS_Horns/BananaSplit</iconPath>
    <renderNodeProperties Inherit="False">
      <li>
        <nodeClass>PawnRenderNode_AttachmentHead</nodeClass>
        <workerClass>PawnRenderNodeWorker_FlipWhenCrawling</workerClass>
        <texPath>Humanlike/HeadAttachments/BS_bananasplit</texPath>
        <color>(255, 255, 255)</color>
        <useSkinShader>false</useSkinShader>
        <colorType>Custom</colorType>
        <drawData>
          <defaultData>
            <layer>80</layer>
          </defaultData>
          <dataNorth>
            <layer>10</layer>
          </dataNorth>
        </drawData>
        <parentTagDef>Head</parentTagDef>
      </li>
    </renderNodeProperties>
  </GeneDef>
