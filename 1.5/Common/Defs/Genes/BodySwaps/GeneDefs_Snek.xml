        </bodyTypeGraphicPaths>
        <drawSize>(1.625,1.625)</drawSize>
        <colorType>Skin</colorType>
        <drawData>
          <dataNorth>
            <offset>(0, 0, -0.3067)</offset>
            <layer>79</layer>
          </dataNorth>
          <dataSouth>
            <offset>(0, -0.0001, -0.3067)</offset>
            <layer>20</layer>
          </dataSouth>
          <dataEast>
            <offset>(-0.3067, -0.0001, -0.3067)</offset>
            <layer>20</layer>
          </dataEast>
          <dataWest>
            <offset>(0.3067, -0.0001, -0.3067)</offset>
            <layer>20</layer>
          </dataWest>
        </drawData>
      </li>
    </renderNodeProperties>
  </GeneDef>
</Defs>