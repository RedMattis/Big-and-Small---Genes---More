<Defs>
  <GeneDef ParentName="BS_BodySwitch">
    <defName>BS_CentaurBody</defName>
    <label>centauroid body</label>
    <description>Carriers of this gene have four legs.</description>
    <biostatCpx>0</biostatCpx>
    <biostatMet>-2</biostatMet>
    <randomChosen>false</randomChosen>
    <iconPath>BS_Icons/Centaur</iconPath>
    <exclusionTags>
      <li>Bug_BodyAttachment</li>
      <li>taur</li>
      <li>BS_BodySwap</li>
    </exclusionTags>
    <modExtensions>
      <li Class="BetterPrerequisites.GeneExtension">
        <thingDefSwap>BS_Centaur</thingDefSwap>
      </li>
    </modExtensions>
  </GeneDef>

  <GeneDef ParentName="BS_BodySwitch">
    <defName>BS_SpiderBody</defName>
    <label>spider-like body</label>
    <description>Carriers of this gene have 8 legs, like a spider.</description>
    <biostatCpx>0</biostatCpx>
    <biostatMet>-2</biostatMet>
    <randomChosen>false</randomChosen>
    <iconPath>BS_Icons/Spider</iconPath>
    <exclusionTags>
      <li>Bug_BodyAttachment</li>
      <li>taur</li>
      <li>BS_BodySwap</li>
    </exclusionTags>
    <modExtensions>
      <li Class="BetterPrerequisites.GeneExtension">
        <thingDefSwap>BS_SpiderPersonRace</thingDefSwap>
      </li>
    </modExtensions>
  </GeneDef>
</Defs>